{
  "productName": "Akso",
  "version": "0.0.1",
  "identifier": "com.hy.akso",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Akso",
        "width": 900,
        "height": 620,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": ["bin/aapt2"],
    "resources": {
      "resources/jadx": "jadx",
      "resources/aapt2": "aapt2"
    }
  }
}
